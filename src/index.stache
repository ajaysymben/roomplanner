<html>
  <head>
    <title>{{title}}</title>
    {{asset "css"}}
    {{asset "html5shiv"}}
  </head>
  <body>
    <can-import from="svg-roomplanner/styles.less!" />
    <can-import from="svg-roomplanner/app" export-as="viewModel" />

    <h1>{{message}}</h1>

    <can-import from="svg-roomplanner/isvg/" />
    <interactive-svg {height}="roomHeight" {width}="roomWidth" {is-running-in-browser}="isRunningInBrowser"></interactive-svg>

    {{asset "inline-cache"}}

    {{#switch env.NODE_ENV}}
      {{#case "production"}}
        <script src="{{joinBase 'node_modules/steal/steal.production.js'}}"  main="svg-roomplanner/index.stache!done-autorender"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js"></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
